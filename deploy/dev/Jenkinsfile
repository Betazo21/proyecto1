pipeline {
    agent any
    
    tools {
        jdk 'java11'
        maven 'maven392'
    }

    environment{
        REGISTRYLOCATION = "https://www.google.com/"
    }
    
    stages {
            stage('Which Java?') {
                steps {
                    sh 'java --version'
                }
            }
            stage ('Build') {
                steps {
                    sh "mvn clean verify"
                }    
            }
    }
}