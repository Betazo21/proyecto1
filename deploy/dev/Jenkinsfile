pipeline {
    agent any
    
    tools {
        jdk 'JAVA11'
        maven 'mv392'
    }

    environment{
        REGISTRYLOCATION = "https://www.google.com/"
    }
    
    stages {
           stage('Initialize'){
            steps{
                script{
                        echo "Inicializando"
                        sh 'java --version'
                        sh "mvn -version"
                    }
                }
             }   
            stage ('Build - Service Usuarios') {
                steps {
                    script{
                       dir("services/service-usuarios/"){
                        sh "mvn package"
                       }
                    }
                }    
            }
    }
}